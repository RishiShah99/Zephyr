{
  "name": "zephyr-dashboard",
  "version": "1.0.0",
  "description": "Beautiful dashboard for Zephyr AI Assistant",
  "main": "electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "build": "vite build",
    "preview": "vite preview"
  },
  "keywords": ["zephyr", "ai", "assistant", "electron", "react"],
  "author": "Rishi",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.294.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "electron": "^28.0.0",
    "vite": "^5.0.0",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6"
  }
}
